<!DOCTYPE html>
<html>
  <head>
    <title>nagano_cake</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <main>

      <div class="container">

        <div class="row">

          <div class="col">

<h1>Admin::Genres#edit</h1>

<h1>ジャンルを編集</h1>

<%= form_with model:@genre,url:admin_genre_path(@genre.id), methiod: :patch do |f| %>

<%= f.text_field :name %>

<%= f.submit'保存'%>

<% end %>

          </div>

        </div>

      </div>

    </main>

  </body>
  
</html>    


