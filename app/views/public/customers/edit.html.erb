

<h1>会員情報編集</h1>

<%= form_with model: @customer, url: public_customer_path(@customer.id), method: :patch do |f| %>

<p>氏名</p>

<p><%= f.text_field :last_name %>  <%= f.text_field:first_name %></p>

<p>カナ</p>

<p><%= f.text_field :last_name_kana %>  <%= f.text_field :first_name_kana %></p>

<p>郵便番号</p>

<p><%= f.text_field :postal_code %></p>

<p>住所</p>

<p><%= f.text_field :address %></p>

<p>電話番号</p>

<p><%= f.text_field :telephone_number %></p>

<p>メールアドレス</p>

<p><%= f.text_field :email %></p>

<%= f.submit '編集内容を保存'%>

<% end %>

<%= link_to'退会する', public_customers_unsubscribe_path %>
